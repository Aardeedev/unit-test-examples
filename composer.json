{
  "name": "tfrommen/unit-test-examples",
  "description": "Example code for the workshop \"An Introduction to Unit Testing (for WordPress)\" at WordCamp Vienna, 2017.",
  "keywords": [
    "code",
    "es6",
    "javascript",
    "mock",
    "mocking",
    "patch",
    "php",
    "test",
    "testable",
    "testing",
    "unit",
    "wordpress",
    "wp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Frommen",
      "email": "info@tfrommen.de",
      "homepage": "https://tfrommen.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/tfrommen/unit-test-examples/issues",
    "source": "https://github.com/tfrommen/unit-test-examples"
  },
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "antecedent/patchwork": "1.3.*",
    "brain/monkey": "^1.4.0",
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.8|~5.1"
  },
  "autoload": {
    "psr-4": {
      "tfrommen\\UnitTestExamples\\": "src/php"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tfrommen\\UnitTestExamples\\Tests\\": "tests/php"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
